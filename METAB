wait()
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayFollow","Head Admin")
wait()
game:GetService("ReplicatedStorage").RemoteEvents.R13PNameE13vent:FireServer("RolePlayFollow","Super admin")
end
